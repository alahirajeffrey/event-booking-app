{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "auth service",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start:prod": "node dist/index.js",
    "start:dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc -p",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
